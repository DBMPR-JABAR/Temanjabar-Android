<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UploadFileView">
        <attr name="uploadType" format="enum">
            <enum name="file" value="1" />
            <enum name="picture" value="2" />
            <enum name="video" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="EmptyStateView">
        <attr name="title_empty" format="string" />
        <attr name="description_empty" format="string" />
    </declare-styleable>
    <declare-styleable name="ErrorStateView">
        <attr name="title_error" format="string" />
        <attr name="description_error" format="string" />
    </declare-styleable>
    <declare-styleable name="SearchTextView">
        <attr name="hint" format="string" />
    </declare-styleable>
</resources>