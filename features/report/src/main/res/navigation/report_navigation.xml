<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/report_navigation"
    app:startDestination="@id/reportFragment2">

    <fragment
        android:id="@+id/reportFragment"
        android:name="id.go.jabarprov.dbmpr.feature.report.presentation.fragments.form_report.MakeReportFragment"
        android:label="fragment_laporan"
        tools:layout="@layout/fragment_make_report">
        <action
            android:id="@+id/action_reportFragment_to_listPhotoFragment"
            app:destination="@id/listPhotoFragment" />
        <action
            android:id="@+id/action_reportFragment_to_videoPlayerFragment"
            app:destination="@id/videoPlayerFragment" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="https://temanjabar.dbmpr.jabarprov.go.id/report" />
    </fragment>

    <fragment
        android:id="@+id/listPhotoFragment"
        android:name="id.go.jabarprov.dbmpr.feature.report.presentation.fragments.ListPhotoFragment"
        android:label="fragment_list_photo"
        tools:layout="@layout/fragment_list_photo">
        <argument
            android:name="listPhoto"
            app:argType="id.go.jabarprov.dbmpr.feature.report.presentation.models.PhotoModel[]" />
    </fragment>
    <fragment
        android:id="@+id/videoPlayerFragment"
        android:name="id.go.jabarprov.dbmpr.feature.report.presentation.fragments.VideoPlayerFragment"
        android:label="fragment_video_player"
        tools:layout="@layout/fragment_video_player">
        <argument android:name="videoUri" />
    </fragment>
    <fragment
        android:id="@+id/listReportFragment"
        android:name="id.go.jabarprov.dbmpr.feature.report.presentation.fragments.list_report.ListReportFragment"
        android:label="fragment_list_report"
        tools:layout="@layout/fragment_list_report">
        <deepLink
            android:id="@+id/deepLink"
            app:uri="https://temanjabar.dbmpr.jabarprov.go.id/list-report" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment2"
        android:name="id.go.jabarprov.dbmpr.feature.report.presentation.fragments.report.ReportFragment"
        android:label="fragment_report"
        tools:layout="@layout/fragment_report" />

</navigation>